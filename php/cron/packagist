LOG_FILE=/var/log/cron.log
SHEL=/bin/bash

*/5 * * * * www-data php /srv/app/console packagist:update --no-debug -v --env=prod >> ${LOG_FILE} 2>&1
*/5 * * * * www-data php /srv/app/console packagist:dump --no-debug -v --env=prod >> ${LOG_FILE} 2>&1
*/5 * * * * www-data php /srv/app/console packagist:index --no-debug -v --env=prod >> ${LOG_FILE} 2>&1
